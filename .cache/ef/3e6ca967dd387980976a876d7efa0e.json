\n      } else if (textAlign === 'center') {\n        point.x -= width / 2;\n      }\n    }\n    if (textBaseline) {\n      if (textBaseline === 'top') {\n        point.y += height;\n      } else if (textBaseline === 'middle') {\n        point.y += height / 2;\n      }\n    }\n\n    bbox = {\n      x: point.x,\n      y: point.y,\n      width: width,\n      height: height,\n    };\n  }\n  return bbox;\n}\n"},"lineCount":null}},"error":null,"hash":"28e238b281b9ac7973ec3d880d165d9c","cacheData":{"env":{}}}